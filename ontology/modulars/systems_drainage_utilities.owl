        <rdfs:comment xml:lang="en">Settling tanks for sediment removal from drainage water.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#WaterTreatmentSystem"/>
