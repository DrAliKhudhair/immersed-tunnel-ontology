    </owl:Class>

    <!-- Drainage Pipes -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#DrainagePipe">
        <rdfs:label xml:lang="en">Drainage Pipe</rdfs:label>
