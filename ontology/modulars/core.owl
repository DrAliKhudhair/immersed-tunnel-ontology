  <!-- =====================================================
       TUNNEL COMPONENTS
       ===================================================== -->
  
  <!-- Base Component Classes -->
  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#TunnelComponent">
    <rdfs:label xml:lang="en">Tunnel Component</rdfs:label>
    <rdfs:comment xml:lang="en">Abstract base class representing discrete physical or functional elements within an immersed tunnel system according to GBT 51318-2019 design framework. Provides foundational semantic structure for component classification, property assignment, and relationships within the tunnel system hierarchy.</rdfs:comment>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#StructuralComponent">
    <rdfs:label xml:lang="en">Structural Component</rdfs:label>
    <rdfs:comment xml:lang="en">Load-bearing structural element designed to provide mechanical resistance and structural integrity within the immersed tunnel system. Characterized by defined load paths, material properties, and capacity for force transmission according to GBT 51318-2019 structural design principles and limit state verification requirements.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelComponent"/>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FunctionalComponent">
    <rdfs:label xml:lang="en">Functional Component</rdfs:label>
    <rdfs:comment xml:lang="en">Non-load-bearing element providing specialized operational functionality including environmental protection, system integration, or service delivery within the immersed tunnel infrastructure. Designed according to GBT 51318-2019 functional performance requirements without primary structural load resistance responsibilities.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelComponent"/>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#TemporaryComponent">
    <rdfs:label xml:lang="en">Temporary Component</rdfs:label>
    <rdfs:comment xml:lang="en">Construction-phase element utilized during fabrication, transportation, or installation processes according to GBT 51318-2019 construction methodology. Designed for removal or replacement upon completion of specific construction stages without compromising permanent structural integrity.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelComponent"/>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PermanentComponent">
    <rdfs:label xml:lang="en">Permanent Component</rdfs:label>
    <rdfs:comment xml:lang="en">Integral element forming part of the final immersed tunnel infrastructure designed for operational service life according to GBT 51318-2019 durability requirements. Characterized by design life compatibility with overall tunnel system performance specifications and maintenance protocols.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelComponent"/>
  </owl:Class>
  
  <!-- Specialized Component Classes -->
  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PipeSectionComponent">
    <rdfs:label xml:lang="en">Pipe Section Component</rdfs:label>
    <rdfs:comment xml:lang="en">Prefabricated tubular structural element constituting primary tunnel enclosure designed according to GBT 51318-2019 element specifications. Characterized by controlled manufacturing tolerances, integrated connection systems, and suitability for floating transportation and underwater installation procedures.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralComponent"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#JointComponent">
    <rdfs:label xml:lang="en">Joint Component</rdfs:label>
    <rdfs:comment xml:lang="en">Structural connection element facilitating continuity between adjacent tunnel segments while providing watertight sealing and load transfer mechanisms. Designed according to GBT 51318-2019 connection requirements with specified deformation compatibility and environmental resistance properties.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralComponent"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#WaterproofingComponent">
    <rdfs:label xml:lang="en">Waterproofing Component</rdfs:label>
    <rdfs:comment xml:lang="en">Functional element providing water barrier protection and sealing systems for tunnel structures according to GBT 51318-2019 waterproofing standards and marine environment requirements.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#FunctionalComponent"/>
  </owl:Class>
  
  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#AntiCorrosionComponent">
    <rdfs:label xml:lang="en">Anti-corrosion Component</rdfs:label>
    <rdfs:comment xml:lang="en">Protective element providing corrosion resistance and material degradation prevention for tunnel structures in marine environments according to GBT 51318-2019 durability requirements.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#FunctionalComponent"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SafetyComponent">
    <rdfs:label xml:lang="en">Safety Component</rdfs:label>
    <rdfs:comment xml:lang="en">Functional element providing safety systems, emergency provisions, and hazard mitigation for tunnel operation according to safety standards and operational requirements.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#FunctionalComponent"/>
  </owl:Class>

  <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ReservedComponent">
    <rdfs:label xml:lang="en">Reserved Component</rdfs:label>
    <rdfs:comment xml:lang="en">Designated space or provision reserved for future equipment installation, system expansion, or operational requirements not specified in initial design phase.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#FunctionalComponent"/>
  </owl:Class>
  
