    <!-- =====================================================
         GBT 51318-2019 SECTION 7 LOAD CLASSIFICATION SYSTEM
         ===================================================== -->

    <!-- Load Classification Base Classes -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#TunnelLoad">
        <rdfs:label xml:lang="en">Tunnel Load</rdfs:label>
        <rdfs:comment xml:lang="en">Comprehensive classification for all loading conditions acting on immersed tunnel structures according to GBT 51318-2019 Section 7.1.1. Loads are systematically categorized into permanent, variable, and accidental classifications based on temporal characteristics, loading patterns, and probability of occurrence throughout the tunnel service life.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralSystem"/>
    </owl:Class>

    <!-- GBT 51318-2019 Table 7.1.1 Load Categories -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad">
        <rdfs:label xml:lang="en">Permanent Load</rdfs:label>
        <rdfs:comment xml:lang="en">Permanent loads that remain essentially constant throughout the service life of the tunnel as defined in GBT 51318-2019 Table 7.1.1. Includes structural self-weight, earth pressure, hydrostatic pressure, concrete shrinkage and creep, loads from structures above, differential settlement, and equipment loads.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#VariableLoad">
        <rdfs:label xml:lang="en">Variable Load</rdfs:label>
        <rdfs:comment xml:lang="en">Variable loads that change in magnitude and location during the service life as defined in GBT 51318-2019 Table 7.1.1. Includes internal traffic load (basic variable), water pressure variation, temperature effects, post-construction settlement, crowd load, surface overload, cable tension, water flow/wave effects, immersion lifting loads, maintenance load, and internal air pressure.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#AccidentalLoad">
        <rdfs:label xml:lang="en">Accidental Load</rdfs:label>
        <rdfs:comment xml:lang="en">Accidental loads with low probability of occurrence but significant structural consequences as defined in GBT 51318-2019 Table 7.1.1. Encompasses seismic load, internal vehicle explosion load, vehicle impact load, anti-terrorism load, ship collision and anchor impact load, and fire load requiring special design consideration for extreme conditions.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelLoad"/>
    </owl:Class>

    <!-- Permanent Load Subclasses (GBT 51318-2019 Table 7.1.1) -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#StructuralSelfWeight">
        <rdfs:label xml:lang="en">Structural Self-Weight</rdfs:label>
        <rdfs:comment xml:lang="en">Self-weight of the tunnel structure based on design dimensions, concrete thickness, protective coatings, and material density as specified in GBT 51318-2019 Section 7.1.3.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#EarthPressure">
        <rdfs:label xml:lang="en">Earth Pressure</rdfs:label>
        <rdfs:comment xml:lang="en">Earth pressure acting on tunnel structure calculated by overburden thickness and unit weight as specified in GBT 51318-2019 Section 7.1.3. Effective weight shall be used for submerged soils.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#HydrostaticPressure">
        <rdfs:label xml:lang="en">Hydrostatic Pressure</rdfs:label>
        <rdfs:comment xml:lang="en">Hydrostatic pressure on tunnel structure calculated by water depth and water unit weight as specified in GBT 51318-2019 Section 7.1.3.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ConcreteShrinkageCreep">
        <rdfs:label xml:lang="en">Concrete Shrinkage and Creep</rdfs:label>
        <rdfs:comment xml:lang="en">Effects of concrete shrinkage and creep on tunnel structure over time as specified in GBT 51318-2019 Table 7.1.1.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SuperstructureLoad">
        <rdfs:label xml:lang="en">Load from Structures Above</rdfs:label>
        <rdfs:comment xml:lang="en">Loads transmitted from structures or buildings above the tunnel as specified in GBT 51318-2019 Table 7.1.1.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#DifferentialSettlementFoundation">
        <rdfs:label xml:lang="en">Differential Settlement of Foundation</rdfs:label>
        <rdfs:comment xml:lang="en">Effects of differential settlement of foundation and base on tunnel structure as specified in GBT 51318-2019 Table 7.1.1.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#EquipmentPrestressedLoad">
        <rdfs:label xml:lang="en">Equipment and Prestressed Concrete Load</rdfs:label>
        <rdfs:comment xml:lang="en">Equipment load and prestressed concrete load acting on tunnel structure as specified in GBT 51318-2019 Table 7.1.1.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#PermanentLoad"/>
    </owl:Class>

    <!-- Variable Load Subclasses - Basic Variable -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#InternalTrafficLoad">
