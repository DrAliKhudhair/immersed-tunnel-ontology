<?xml version='1.0' encoding='utf-8'?>
<rdf:RDF xmlns="http://www.semanticweb.org/immersedTunnelOnto#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="http://www.semanticweb.org/immersedTunnelOnto/modulars/structural_systems">
    <rdfs:label xml:lang="en">Immersed Tunnel Ontology - Structural Systems Module</rdfs:label>
    <rdfs:comment xml:lang="en">
      Structural systems module providing comprehensive classification and specifications for immersed tunnel structural design.
      Aligned with GBT 51318-2019 "Immersed Tunnel Design Standard" covering structural design, joint systems, and foundation integration.
      
      This module encompasses:
      - Settlement-based joint design classes for IMMTD05
      - Longitudinal structural system design for IMMTD06
      - Tunnel segment geometry and specifications
      - Joint design and waterproofing requirements
      - Foundation integration requirements
      
      STANDARDS ALIGNMENT:
      - Structural design methodology per GBT 51318-2019
      - Joint design requirements and specifications
      - Segment division logic and optimization
    </rdfs:comment>
    <owl:versionInfo>4.0.0-GBT51318-2019-enhanced</owl:versionInfo>
  </owl:Ontology>

    <!-- =====================================================
         IMMTD05 SETTLEMENT-BASED JOINT DESIGN CLASSES
         ===================================================== -->

    <!-- Settlement-Based Joint Design Classes -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SettlementBasedJointDesign">
        <rdfs:label xml:lang="en">Settlement Based Joint Design</rdfs:label>
        <rdfs:comment xml:lang="en">Joint design requirements based on settlement and deformation behavior for IMMTD05.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralJoint"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#DeformationAccommodation">
        <rdfs:label xml:lang="en">Deformation Accommodation</rdfs:label>
        <rdfs:comment xml:lang="en">Joint system design for accommodating angular, longitudinal, and transverse deformation under operational and settlement conditions.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SettlementBasedJointDesign"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#JointPositioningRequirement">
        <rdfs:label xml:lang="en">Joint Positioning Requirement</rdfs:label>
        <rdfs:comment xml:lang="en">Joint positioning requirements based on geological conditions and spacing criteria.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SettlementBasedJointDesign"/>
    </owl:Class>

    <!-- Segment Layout Optimization -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SegmentLayoutOptimization">
        <rdfs:label xml:lang="en">Segment Layout Optimization</rdfs:label>
        <rdfs:comment xml:lang="en">Tunnel segment layout optimization considering joint positions, settlement compensation, and geological conditions.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralDesign"/>
    </owl:Class>

