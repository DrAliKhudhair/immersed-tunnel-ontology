    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#AntiTerrorismLoad">
        <rdfs:label xml:lang="en">Anti-Terrorism Load</rdfs:label>
        <rdfs:comment xml:lang="en">Anti-terrorism loads per Ministry of Public Security standards as specified in GBT 51318-2019 Table 7.1.1 and Section 7.1.5.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#AccidentalLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ShipCollisionAnchorImpact">
        <rdfs:label xml:lang="en">Ship Collision/Anchor Impact Load</rdfs:label>
        <rdfs:comment xml:lang="en">Ship collision and anchor impact loads based on navigation planning, cover thickness, and water depth as specified in GBT 51318-2019 Table 7.1.1 and Section 7.1.5.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#AccidentalLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FireLoad">
        <rdfs:label xml:lang="en">Fire Load</rdfs:label>
        <rdfs:comment xml:lang="en">Fire load effects on tunnel structure as specified in GBT 51318-2019 Table 7.1.1.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#AccidentalLoad"/>
    </owl:Class>

    <!-- =====================================================
         GBT 51318-2019 SECTION 7.2 LOAD COMBINATION SYSTEM
         ===================================================== -->

    <!-- Load Combination Base Classes -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#LoadCombination">
        <rdfs:label xml:lang="en">Load Combination</rdfs:label>
        <rdfs:comment xml:lang="en">Base class for load combinations considering simultaneous occurrence of loads during construction or operation using both ultimate and serviceability limit states as specified in GBT 51318-2019 Section 7.2.1.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralSystem"/>
    </owl:Class>

    <!-- Limit State Classes -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#UltimateLimitState">
        <rdfs:label xml:lang="en">Ultimate Limit State</rdfs:label>
        <rdfs:comment xml:lang="en">Ultimate limit state design verification using γ₀Sd ≤ Rd formula where γ₀ is structural importance factor, Sd is load effect design value, and Rd is structural resistance design value as specified in GBT 51318-2019 Section 7.2.2.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#LoadCombination"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ServiceabilityLimitState">
        <rdfs:label xml:lang="en">Serviceability Limit State</rdfs:label>
        <rdfs:comment xml:lang="en">Serviceability limit state design verification using Sd ≤ C formula where Sd is load effect design value and C is allowable limit value for structure or component as specified in GBT 51318-2019 Section 7.2.6.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#LoadCombination"/>
    </owl:Class>

    <!-- Structural Importance Classification -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#StructuralImportanceLevel">
        <rdfs:label xml:lang="en">Structural Importance Level</rdfs:label>
        <rdfs:comment xml:lang="en">Structural importance level classification affecting importance factor γ₀ in ultimate limit state design as specified in GBT 51318-2019 Section 7.2.3.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralSystem"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#Level1SafetyStructure">
        <rdfs:label xml:lang="en">Level 1 Safety Structure (Main Structure)</rdfs:label>
        <rdfs:comment xml:lang="en">Main structure with Level 1 safety classification requiring structural importance factor γ₀ ≥ 1.1 as specified in GBT 51318-2019 Section 7.2.3.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralImportanceLevel"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#Level2SafetyStructure">
        <rdfs:label xml:lang="en">Level 2 Safety Structure (Secondary Structure)</rdfs:label>
        <rdfs:comment xml:lang="en">Secondary structure with Level 2 safety classification requiring structural importance factor γ₀ ≥ 1.0 as specified in GBT 51318-2019 Section 7.2.3.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralImportanceLevel"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#Level3SafetyStructure">
        <rdfs:label xml:lang="en">Level 3 Safety Structure (Temporary Structure)</rdfs:label>
        <rdfs:comment xml:lang="en">Temporary structure with Level 3 safety classification requiring structural importance factor γ₀ ≥ 0.9 as specified in GBT 51318-2019 Section 7.2.3.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#StructuralImportanceLevel"/>
    </owl:Class>

    <!-- Load Combination Types -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#BasicCombination">
        <rdfs:label xml:lang="en">Basic Combination</rdfs:label>
        <rdfs:comment xml:lang="en">Basic load combination for ultimate limit state with partial safety factors: permanent loads (unfavorable: 1.2, favorable: ≤1.0) and variable loads (1.4) as specified in GBT 51318-2019 Section 7.2.4.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#UltimateLimitState"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#AccidentalCombination">
        <rdfs:label xml:lang="en">Accidental Combination</rdfs:label>
        <rdfs:comment xml:lang="en">Accidental load combination for ultimate limit state using formula: Sd = ΣSGk + SAd + ψf1SQ1k + Σψqi SQk as specified in GBT 51318-2019 Section 7.2.5.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#UltimateLimitState"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PostDamageCombination">
        <rdfs:label xml:lang="en">Post-Damage Combination</rdfs:label>
        <rdfs:comment xml:lang="en">Post-damage load combination after accidental events using formula: Sd = ΣSGk + ψf1SQ1k + Σψqi SQk as specified in GBT 51318-2019 Section 7.2.5.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#UltimateLimitState"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#StandardCombination">
        <rdfs:label xml:lang="en">Standard Combination</rdfs:label>
        <rdfs:comment xml:lang="en">Standard combination for serviceability limit state using formula: Sd = ΣSGk + SQ1k + Σψci SQk as specified in GBT 51318-2019 Section 7.2.7.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ServiceabilityLimitState"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FrequentCombination">
        <rdfs:label xml:lang="en">Frequent Combination</rdfs:label>
        <rdfs:comment xml:lang="en">Frequent combination for serviceability limit state using formula: Sd = ΣSGk + ψf1SQ1k + Σψqi SQk as specified in GBT 51318-2019 Section 7.2.8.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ServiceabilityLimitState"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#QuasiPermanentCombination">
        <rdfs:label xml:lang="en">Quasi-Permanent Combination</rdfs:label>
        <rdfs:comment xml:lang="en">Quasi-permanent combination for serviceability limit state using formula: Sd = ΣSGk + Σψqi SQk as specified in GBT 51318-2019 Section 7.2.9.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ServiceabilityLimitState"/>
    </owl:Class>

    <!-- Representative Values Classification -->
    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#LoadRepresentativeValue">
        <rdfs:label xml:lang="en">Load Representative Value</rdfs:label>
        <rdfs:comment xml:lang="en">Representative values for load design including standard value, combination value, frequent value, and quasi-permanent value as specified in GBT 51318-2019 Section 7.1.6.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelLoad"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#StandardValue">
        <rdfs:label xml:lang="en">Standard Value</rdfs:label>
        <rdfs:comment xml:lang="en">Standard value as load representative value based on probability analysis or national standards as specified in GBT 51318-2019 Section 7.1.6.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#LoadRepresentativeValue"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#CombinationValue">
        <rdfs:label xml:lang="en">Combination Value</rdfs:label>
        <rdfs:comment xml:lang="en">Combination value calculated as standard value × combination coefficient (ψc) for ultimate and serviceability limit states as specified in GBT 51318-2019 Section 7.1.7.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#LoadRepresentativeValue"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FrequentValue">
        <rdfs:label xml:lang="en">Frequent Value</rdfs:label>
        <rdfs:comment xml:lang="en">Frequent value calculated as standard value × frequency coefficient (ψf) for frequent combinations as specified in GBT 51318-2019 Section 7.1.8.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#LoadRepresentativeValue"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#QuasiPermanentValue">
        <rdfs:label xml:lang="en">Quasi-Permanent Value</rdfs:label>
        <rdfs:comment xml:lang="en">Quasi-permanent value calculated as standard value × quasi-permanent coefficient (ψq) for quasi-permanent combinations as specified in GBT 51318-2019 Section 7.1.8.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#LoadRepresentativeValue"/>
    </owl:Class>
</rdf:RDF> 