<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/immersedTunnelOnto#"
     xml:base="http://www.semanticweb.org/immersedTunnelOnto"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/immersedTunnelOnto/modulars/site_environmental_systems">
        <rdfs:label xml:lang="en">Immersed Tunnel Ontology - Site and Environmental Systems</rdfs:label>
        <rdfs:comment xml:lang="en">
            Site characterization and environmental systems module for immersed tunnel engineering aligned with GBT 51318-2019.
            Defines comprehensive framework for site investigation, environmental assessment, and monitoring systems.
            
            STANDARDS ALIGNMENT:
            - GBT 51318-2019 Chapter 4: Engineering Survey and Site Investigation methodology
            - Section 4.3: Phased tunnel investigation framework and requirements
            - Section 4.2.2: Comprehensive survey data type specifications
            - Investigation phases: feasibility study, preliminary, detailed, and supplemental
            
            INVESTIGATION FRAMEWORK:
            - Phased investigation approach matching design stages
            - Subaqueous topographic mapping with scale-specific requirements
            - Water area investigation with systematic borehole arrangements
            - Hydrological investigation including flow velocity and water characteristics
            - Integration of underwater and terrestrial survey methodologies
            
            MODULE SCOPE:
            - Site investigation classification and methodology framework
            - Environmental assessment and monitoring system taxonomy
            - Survey data type definitions aligned with standard requirements
            - Environmental protection and sustainability system architecture
            - Design parameter derivation from investigation data
        </rdfs:comment>
        <owl:versionInfo>3.3.0-ENHANCED-GBT51318-2019-standards-compliant</owl:versionInfo>
        <owl:imports rdf:resource="http://www.semanticweb.org/immersedTunnelOnto/modulars/core"/>
    </owl:Ontology>

    <!-- =====================================================
         BASE ENVIRONMENTAL CLASSES
         ===================================================== -->

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#EnvironmentalSystem">
        <rdfs:label xml:lang="en">Environmental System</rdfs:label>
        <rdfs:comment xml:lang="en">Systematic framework encompassing all environmental aspects, site conditions, and ecological considerations for immersed tunnel engineering.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#TunnelComponent"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SiteSystem">
        <rdfs:label xml:lang="en">Site System</rdfs:label>
        <rdfs:comment xml:lang="en">Comprehensive framework for site characterization, investigation, and survey methodologies supporting tunnel design and construction planning.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#EnvironmentalSystem"/>
    </owl:Class>

    <!-- =====================================================
         PHASED SITE INVESTIGATION FRAMEWORK (GBT 51318-2019 Section 4.3.1)
         ===================================================== -->

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SiteInvestigation">
        <rdfs:label xml:lang="en">Site Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Systematic engineering survey and site investigation framework conducted according to technical features, design requirements, and regional conditions of immersed tunnels as specified in GBT 51318-2019 Section 4.3.1. Investigation phases match design stages and ensure accurate and reliable data collection with precision complying with relevant national standards.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SiteSystem"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FeasibilityStudyInvestigation">
        <rdfs:label xml:lang="en">Feasibility Study Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Initial phase investigation conducted based on data collection and site visits as specified in GBT 51318-2019 Section 4.3.3. Includes borehole spacing of 400-500m with minimum 2 boreholes and at least one borehole per construction unit or tunnel alignment section. Provides essential data for project viability assessment and preliminary site reconnaissance.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SiteInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PreliminaryInvestigation">
        <rdfs:label xml:lang="en">Preliminary Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Intermediate phase investigation providing detailed site characterization for preliminary design development as specified in GBT 51318-2019 Section 4.3.3. Requires minimum 5 boreholes in complex geological areas with 100-300m spacing. General boreholes penetrate 1.5× tunnel height below base while control boreholes reach 2.5× tunnel height in soft soils.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SiteInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#DetailedInvestigation">
        <rdfs:label xml:lang="en">Detailed Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Comprehensive investigation phase providing precise site data for final design development and construction documentation as specified in GBT 51318-2019 Section 4.3.3. Utilizes staggered grid arrangement with 30-50m spacing in joint-affected areas and 40-60m spacing on excavation slopes. Essential for construction drawing design stage requirements.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SiteInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SupplementalInvestigation">
        <rdfs:label xml:lang="en">Supplemental Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Additional investigation conducted as necessary to address specific design requirements or resolve identified uncertainties.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SiteInvestigation"/>
    </owl:Class>

    <!-- =====================================================
         SUBAQUEOUS TOPOGRAPHIC MAPPING (GBT 51318-2019 Section 4.3.2)
         ===================================================== -->

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SubaqueousTopographicMapping">
        <rdfs:label xml:lang="en">Subaqueous Topographic Mapping</rdfs:label>
        <rdfs:comment xml:lang="en">Systematic underwater topographic survey methodology ensuring seamless integration of underwater and terrestrial measurements as specified in GBT 51318-2019 Section 4.3.2. Scale requirements vary by design stage: feasibility study based on data collection meeting regulatory approval precision, preliminary design at 1:1000-1:2000 scale with 2-3km coverage, and construction drawing design at 1:500-1:1000 scale with 0.5-1km coverage.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SiteInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FeasibilityStageMapping">
        <rdfs:label xml:lang="en">Feasibility Stage Mapping</rdfs:label>
        <rdfs:comment xml:lang="en">Topographic mapping during feasibility study based on data collection and site surveys, with precision meeting regulatory approval requirements.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SubaqueousTopographicMapping"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PreliminaryDesignMapping">
        <rdfs:label xml:lang="en">Preliminary Design Stage Mapping</rdfs:label>
        <rdfs:comment xml:lang="en">Topographic mapping for preliminary design with scale range 1:1000 to 1:2000 and coverage extending 2-3 km on both sides of tunnel alignment.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SubaqueousTopographicMapping"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ConstructionDrawingMapping">
        <rdfs:label xml:lang="en">Construction Drawing Stage Mapping</rdfs:label>
        <rdfs:comment xml:lang="en">Detailed topographic mapping for construction documentation with scale range 1:500 to 1:1000 and coverage extending 0.5-1 km on both sides of tunnel alignment.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SubaqueousTopographicMapping"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FloatingTransportationAreaMapping">
        <rdfs:label xml:lang="en">Floating Transportation Area Mapping</rdfs:label>
        <rdfs:comment xml:lang="en">Extended mapping coverage including element floating transportation area when utilizing movable dry dock construction methodology.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SubaqueousTopographicMapping"/>
    </owl:Class>

    <!-- =====================================================
         WATER AREA INVESTIGATION (GBT 51318-2019 Section 4.3.3)
         ===================================================== -->

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#WaterAreaInvestigation">
        <rdfs:label xml:lang="en">Water Area Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Systematic underwater investigation methodology with phase-specific borehole spacing and depth requirements for comprehensive subsurface characterization.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SiteInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#BoreholeInvestigation">
        <rdfs:label xml:lang="en">Borehole Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Systematic drilling investigation for subsurface exploration with phase-specific spacing, depth, and arrangement requirements.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#WaterAreaInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FeasibilityStageBoreholes">
        <rdfs:label xml:lang="en">Feasibility Stage Boreholes</rdfs:label>
        <rdfs:comment xml:lang="en">Initial borehole investigation with 400-500m spacing, minimum 2 boreholes, and depth requirements based on soil conditions and tunnel dimensions.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#BoreholeInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PreliminaryStageBoreholes">
        <rdfs:label xml:lang="en">Preliminary Stage Boreholes</rdfs:label>
        <rdfs:comment xml:lang="en">Intermediate borehole investigation with 100-300m spacing, minimum 5 boreholes in complex areas, with general and control borehole depth specifications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#BoreholeInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#DetailedStageBoreholes">
        <rdfs:label xml:lang="en">Detailed Stage Boreholes</rdfs:label>
        <rdfs:comment xml:lang="en">Comprehensive borehole investigation using staggered grid arrangement with specific spacing for joint-affected areas (30-50m) and excavation slopes (40-60m).</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#BoreholeInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SoftSoilBoreholes">
        <rdfs:label xml:lang="en">Soft Soil Boreholes</rdfs:label>
        <rdfs:comment xml:lang="en">Borehole investigation in soft soil conditions with depth requirements based on tunnel height multiples and minimum depth specifications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#BoreholeInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#WeatheredRockBoreholes">
        <rdfs:label xml:lang="en">Weathered Rock Boreholes</rdfs:label>
        <rdfs:comment xml:lang="en">Borehole investigation in slightly or moderately weathered rock formations with specific depth requirements and cavity penetration protocols.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#BoreholeInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SiltCoveredBottomInvestigation">
        <rdfs:label xml:lang="en">Silt-Covered Bottom Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Specialized investigation for river or sea bottoms covered by silt, including thickness measurement and sediment density determination.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#WaterAreaInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FloatingZoneTrenchingInvestigation">
        <rdfs:label xml:lang="en">Floating Zone Trenching Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Investigation within element floating zones requiring trenching, with borehole arrangement based on regional geology and construction calculation requirements.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#WaterAreaInvestigation"/>
    </owl:Class>

