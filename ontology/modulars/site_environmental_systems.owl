    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ConnectionSegmentScheme">
        <rdfs:label xml:lang="en">Connection Segment Scheme</rdfs:label>
        <rdfs:comment xml:lang="en">Connection segment scheme at tunnel ends matching dry dock scheme and prefabrication schedule requirements.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ControlledTechnicalScheme"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FloatingTransportationScheme">
        <rdfs:label xml:lang="en">Floating Transportation Scheme</rdfs:label>
        <rdfs:comment xml:lang="en">Floating transportation method determination based on dock type, navigation conditions, floating distance, hydrology, and meteorological factors.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ControlledTechnicalScheme"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ImmersionScheme">
        <rdfs:label xml:lang="en">Immersion Scheme</rdfs:label>
        <rdfs:comment xml:lang="en">Element immersion method determination based on navigation conditions, hydrological environment, element structure, and construction equipment.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ControlledTechnicalScheme"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#FinalJointScheme">
        <rdfs:label xml:lang="en">Final Joint Scheme</rdfs:label>
        <rdfs:comment xml:lang="en">Final joint location and structural form determination based on navigation conditions, tunnel depth, dry dock scheme, and construction schedule.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ControlledTechnicalScheme"/>
    </owl:Class>

    <!-- =====================================================
         MATERIALS SPECIFICATION FRAMEWORK (GBT 51318-2019 Section 6)
         ===================================================== -->

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#MaterialsSpecificationSystem">
        <rdfs:label xml:lang="en">Materials Specification System</rdfs:label>
        <rdfs:comment xml:lang="en">Comprehensive materials specification framework based on structural type, load conditions, usage requirements, and environmental factors as specified in GBT 51318-2019 Section 6.0.1. Ensures selection of engineering materials meets safety, reliability, technical advancement, economic reasonableness, and durability principles established in the standard's general provisions.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#EnvironmentalSystem"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ConcreteSpecification">
        <rdfs:label xml:lang="en">Concrete Specification</rdfs:label>
        <rdfs:comment xml:lang="en">Concrete specification framework including waterproof concrete requirements, impermeability grades, durability requirements, and strength classifications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#MaterialsSpecificationSystem"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#MainStructuralConcrete">
        <rdfs:label xml:lang="en">Main Structural Concrete</rdfs:label>
        <rdfs:comment xml:lang="en">Main structural concrete specification for immersed tunnel elements adopting waterproof concrete with impermeability grade not lower than P8 as specified in GBT 51318-2019 Section 6.0.2. Minimum strength grade shall not be lower than C35 per Section 6.0.4. Raw materials and mix proportions must meet durability requirements including minimum strength, maximum water-cement ratio, minimum cementitious material content, and requirements for crack resistance, impermeability, freeze-thaw resistance, and corrosion resistance.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ConcreteSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PrestressedConcrete">
        <rdfs:label xml:lang="en">Prestressed Concrete</rdfs:label>
        <rdfs:comment xml:lang="en">Prestressed concrete specification for immersed tunnel element sections with minimum strength grade not lower than C40 as specified in GBT 51318-2019 Section 6.0.4. Must meet enhanced durability requirements and structural performance criteria for prestressed construction applications in underwater environment.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ConcreteSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#MicroExpansiveConcrete">
        <rdfs:label xml:lang="en">Micro-Expansive Concrete</rdfs:label>
        <rdfs:comment xml:lang="en">Micro-expansive concrete specification for backfill behind elements with impermeability and strength not lower than main structural concrete.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ConcreteSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#BallastTankConcrete">
        <rdfs:label xml:lang="en">Ballast Tank Concrete</rdfs:label>
        <rdfs:comment xml:lang="en">Ballast tank concrete specification with minimum design strength grade C25 for structural applications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ConcreteSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#AntiCorrosionConcrete">
        <rdfs:label xml:lang="en">Anti-Corrosion Concrete</rdfs:label>
        <rdfs:comment xml:lang="en">Anti-corrosion concrete layer specification with minimum strength grade C20 for protective applications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#ConcreteSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SteelSpecification">
        <rdfs:label xml:lang="en">Steel Specification</rdfs:label>
        <rdfs:comment xml:lang="en">Steel specification framework including structural steel, reinforcement, and specialized steel components with grade classifications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#MaterialsSpecificationSystem"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#StructuralSteel">
        <rdfs:label xml:lang="en">Structural Steel</rdfs:label>
        <rdfs:comment xml:lang="en">Structural steel specification for immersed tunnel element end walls, watertight doors, shear keys, and end closure components using Q235B grade as specified in GBT 51318-2019 Section 6.0.7. Ensures structural integrity and waterproofing performance in underwater construction applications.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SteelSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#ReinforcementSteel">
        <rdfs:label xml:lang="en">Reinforcement Steel</rdfs:label>
        <rdfs:comment xml:lang="en">Reinforcement steel specification with main reinforcement bars using HRB400 or HRB500 grades and stirrups using HPB300 or HRB400 grades as specified in GBT 51318-2019 Section 6.0.8. Ensures adequate structural reinforcement performance in immersed tunnel concrete elements.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SteelSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#SpecializedMaterialSpecification">
        <rdfs:label xml:lang="en">Specialized Material Specification</rdfs:label>
        <rdfs:comment xml:lang="en">Specialized material specification framework including grouting materials, prestressing tendons, waterstops, and support components.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#MaterialsSpecificationSystem"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#GroutingMaterial">
        <rdfs:label xml:lang="en">Grouting Material</rdfs:label>
        <rdfs:comment xml:lang="en">Non-shrink grouting material specification for element end sealing injection with minimum strength grade M25.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SpecializedMaterialSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#PrestressingTendon">
        <rdfs:label xml:lang="en">Prestressing Tendon</rdfs:label>
        <rdfs:comment xml:lang="en">High-strength low-relaxation steel strand specification for PC tendons with minimum ultimate strength 1860 MPa.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SpecializedMaterialSpecification"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#WaterstopSpecification">
        <rdfs:label xml:lang="en">Waterstop Specification</rdfs:label>
        <rdfs:comment xml:lang="en">GINA and OMEGA waterstop specification with associated pressure plates and steel components meeting durability requirements.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#SpecializedMaterialSpecification"/>
    </owl:Class>

