        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#HydrologicalInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#WaterLevelInvestigation">
        <rdfs:label xml:lang="en">Water Level Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Investigation of water level variations including tidal ranges and seasonal fluctuations affecting tunnel positioning and structural loads.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#HydrologicalInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#WaterDensityInvestigation">
        <rdfs:label xml:lang="en">Water Density Investigation</rdfs:label>
        <rdfs:comment xml:lang="en">Investigation of water density characteristics affecting buoyancy calculations and element flotation design parameters.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#HydrologicalInvestigation"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/immersedTunnelOnto#RelatedHydrologicalParameters">
        <rdfs:label xml:lang="en">Related Hydrological Parameters</rdfs:label>
        <rdfs:comment xml:lang="en">Additional hydrological parameters relevant to tunnel design including temperature variations, salinity, and chemical composition affecting materials and construction.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/immersedTunnelOnto#HydrologicalInvestigation"/>
    </owl:Class>

