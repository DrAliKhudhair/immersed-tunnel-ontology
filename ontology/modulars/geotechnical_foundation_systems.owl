<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/immersedTunnelOnto#"
     xml:base="http://www.semanticweb.org/immersedTunnelOnto"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/immersedTunnelOnto/modulars/geotechnical_foundation_systems">
        <rdfs:label xml:lang="en">Immersed Tunnel Ontology - Geotechnical and Foundation Systems</rdfs:label>
        <rdfs:comment xml:lang="en">
            Enhanced geotechnical analysis and foundation systems for immersed tunnels.
            Comprehensive soil analysis, foundation design, and ground improvement.
            Based on GB/T 51318-2019 and JTG 2120-2020 Chapter 9-10 standards.
            
            CONSOLIDATES:
            - Geotechnical analysis from geotechnical_analysis.owl
            - Foundation systems from foundation_systems_components.owl
            - Soil investigation and characterization
            - Ground improvement and treatment methods
            
            ENHANCEMENTS:
            - Advanced geotechnical modeling
            - Foundation optimization
            - Ground treatment strategies
            - Stability analysis methods
        </rdfs:comment>
        <owl:versionInfo>3.0.0 - Enhanced</owl:versionInfo>
        <owl:imports rdf:resource="http://www.semanticweb.org/immersedTunnelOnto/modulars/core"/>
    </owl:Ontology>

    <!-- Base Classes -->
